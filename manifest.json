{
    "name": "YandexMusic-DiscordRP Browser Extension",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage",
        "https://music.yandex.ru/",
        "https://music.yandex.kz/",
        "https://music.yandex.com/",
        "https://music.yandex.com/*",
        "https://music.yandex.ru/*",
        "https://music.yandex.kz/*",
        "https://music.yandex.com/*/*",
        "https://music.yandex.ru/*/*",
        "https://music.yandex.kz/*/*",
        "https://music.yandex.com/*/*/*",
        "https://music.yandex.ru/*/*/*",
        "https://music.yandex.kz/*/*/*",
        "https://music.yandex.com/*/*/*/*",
        "https://music.yandex.ru/*/*/*/*",
        "https://music.yandex.kz/*/*/*/*",
        "https://music.yandex.com/*/*/*/*/*",
        "https://music.yandex.ru/*/*/*/*/*",
        "https://music.yandex.kz/*/*/*/*/*"
    ],
    "icons": {
        "16": "/img/16.png",
        "48": "/img/48.png",
        "128": "/img/128.png"
    },
    "web_accessible_resources": [
        "tracker.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://music.yandex.ru/",
                "https://music.yandex.kz/",
                "https://music.yandex.com/",
                "https://music.yandex.com/*",
                "https://music.yandex.ru/*",
                "https://music.yandex.kz/*",
                "https://music.yandex.com/*/*",
                "https://music.yandex.ru/*/*",
                "https://music.yandex.kz/*/*",
                "https://music.yandex.com/*/*/*",
                "https://music.yandex.ru/*/*/*",
                "https://music.yandex.kz/*/*/*",
                "https://music.yandex.com/*/*/*/*",
                "https://music.yandex.ru/*/*/*/*",
                "https://music.yandex.kz/*/*/*/*",
                "https://music.yandex.com/*/*/*/*/*",
                "https://music.yandex.ru/*/*/*/*/*",
                "https://music.yandex.kz/*/*/*/*/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "externally_connectable": {
        "matches": [
            "https://music.yandex.ru/",
            "https://music.yandex.kz/",
            "https://music.yandex.com/",
            "https://music.yandex.com/*",
            "https://music.yandex.ru/*",
            "https://music.yandex.kz/*",
            "https://music.yandex.com/*/*",
            "https://music.yandex.ru/*/*",
            "https://music.yandex.kz/*/*",
            "https://music.yandex.com/*/*/*",
            "https://music.yandex.ru/*/*/*",
            "https://music.yandex.kz/*/*/*",
            "https://music.yandex.com/*/*/*/*",
            "https://music.yandex.ru/*/*/*/*",
            "https://music.yandex.kz/*/*/*/*",
            "https://music.yandex.com/*/*/*/*/*",
            "https://music.yandex.ru/*/*/*/*/*",
            "https://music.yandex.kz/*/*/*/*/*"
        ]
    },
    "browser_action": {
        "default_title": "YandexMusic-DiscordRP",
        "default_popup": "popup.html"
    }
}